function findDuplicate(arr) {
    const duplicateArr = [];
    const seen = new Set();
    const added = new Set();

    for (var i= 0; i < arr.length; i++) {
        if (seen.has(arr[i]) && !added.has(arr[i])) {
            duplicateArr.push(arr[i]);
            added.add(arr[i]);
        } else {
            seen.add(arr[i]);
        }
    }

    return duplicateArr;
}

const data = [10,20,20,30,30,40,40,50];
console.log(findDuplicate(data)); 